{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php-symfony": "^2.2",
        "sensio/framework-extra-bundle": "^5.5",
        "sensiolabs/security-checker": "^6.0",
        "shivas/versioning-bundle": "^3.2",
        "smalot/pdfparser": "^0.15.0",
        "spatie/pdf-to-image": "^2.0",
        "symfony/amazon-mailer": "5.0.*",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.6",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/mailer": "5.0.*",
        "symfony/maker-bundle": "^1.15",
        "symfony/notifier": "5.0.*",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "5.0.*",
        "symfony/sendgrid-mailer": "5.0.*",
        "symfony/slack-notifier": "5.0.*",
        "symfony/twig-pack": "^1.0",
        "symfony/twilio-notifier": "5.0.*",
        "symfony/validator": "5.0.*",
        "symfony/var-dumper": "5.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.0.*",
        "thiagoalessio/tesseract_ocr": "^2.9",
        "twig/cssinliner-extra": "^3.0"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
